    src url("/backnn/fonts/Akkurat-Lightitalic-Web/lineto-akkurat-lightitalic.eot?#iefix") format("embedded-opentype"),
        url("/backnn/fonts/Akkurat-Lightitalic-Web/lineto-akkurat-lightitalic.woff") format("woff")
    font-weight 300
    font-style italic

@font-face

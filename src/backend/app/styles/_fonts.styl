    src url("/backnn/fonts/Akkurat-Bold-Web/lineto-akkurat-bold.eot")
    src url("/backnn/fonts/Akkurat-Bold-Web/lineto-akkurat-bold.eot?#iefix") format("embedded-opentype"),
        url("/backnn/fonts/Akkurat-Bold-Web/lineto-akkurat-bold.woff") format("woff")
    font-weight 600
    font-style normal

@font-face
    font-family "Akkurat-Web"
    src url("/backnn/fonts/Akkurat-Light-Web/lineto-akkurat-light.eot")
    src url("/backnn/fonts/Akkurat-Light-Web/lineto-akkurat-light.eot?#iefix") format("embedded-opentype"),
        url("/backnn/fonts/Akkurat-Light-Web/lineto-akkurat-light.woff") format("woff")
    font-weight 300
    font-style normal

@font-face
    font-family "Akkurat-Web"
